{
  "name": "express-server",
  "version": "0.0.1",
  "scripts": {
    "lint-check": "npx eslint .",
    "lint-fix": "npx eslint --fix .",
    "lint-fix-staged": "npx lint-staged --verbose",
    "start": "cd example && node start",
    "console": "cd example && node console",
    "test": "cd example && npm run test"
  },
  "dependencies": {
    "ejs": "3.1.6",
    "underscore": "1.13.1",
    "underscore.string": "3.3.5",
    "yaml-config": "0.3.0",
    "express": "4.17.1",
    "moment": "2.29.1",
    "glob": "7.1.7"
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "standard": "^16.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": "npx eslint --fix"
  }
}
